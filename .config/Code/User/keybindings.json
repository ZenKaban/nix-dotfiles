// Place your key bindings in this file to override the defaults
[
    {
        "key": "ctrl+shift+[Slash]",
        "command": "vscode-neovim.send",
        "when": "editorTextFocus && neovim.ctrlKeysNormal./ && neovim.init && neovim.mode != 'insert' && editorLangId not in 'neovim.editorLangIdExclusions'"
    },
    {
        "key": "ctrl+[IntlBackslash]",
        "command": "-vscode-neovim.send",
        "when": "editorTextFocus && neovim.ctrlKeysNormal./ && neovim.init && neovim.mode != 'insert' && editorLangId not in 'neovim.editorLangIdExclusions'"
    },
    {
        "key": "ctrl+[IntlBackslash]",
        "command": "-vscode-neovim.send",
        "when": "editorTextFocus && neovim.ctrlKeysInsert./ && neovim.init && neovim.mode == 'insert' && editorLangId not in 'neovim.editorLangIdExclusions'"
    },
    {
        "key": "ctrl+shift+alt+meta+s ctrl+shift+alt+meta+r",
        "command": "yandex-code-assist.reviewChanges.stagedDiff"
    },
    {
        "key": "ctrl+shift+alt+meta+e",
        "command": "workbench.view.explorer",
        "when": "viewContainer.workbench.view.explorer.enabled"
    },
    {
        "key": "ctrl+shift+e",
        "command": "-workbench.view.explorer",
        "when": "viewContainer.workbench.view.explorer.enabled"
    },
    {
        "key": "ctrl+shift+alt+meta+a ctrl+shift+alt+meta+s",
        "command": "arc.stageAll"
    },
    {
        "key": "ctrl+shift+alt+meta+a ctrl+shift+alt+meta+c",
        "command": "arc.commitWithInput"
    },
    {
        "key": "ctrl+shift+alt+meta+a ctrl+shift+alt+meta+b",
        "command": "arc.checkout"
    },
    {
        "key": "ctrl+shift+b",
        "command": "-workbench.action.tasks.build",
        "when": "taskCommandsRegistered"
    },
    {
        "key": "ctrl+shift+alt+meta+b",
        "command": "workbench.action.toggleAuxiliaryBar"
    },
    {
        "key": "ctrl+alt+b",
        "command": "-workbench.action.toggleAuxiliaryBar"
    },
    {
        "key": "ctrl+shift+alt+meta+n",
        "command": "workbench.action.moveEditorToNewWindow"
    },
    {
        "key": "ctrl+shift+alt+meta+a ctrl+shift+alt+meta+p",
        "command": "arcanum.openPullRequestInBrowser"
    },
    {
        "key": "ctrl+shift+alt+meta+q",
        "command": "workbench.view.extension.arcanumPullRequests"
    },
    {
        "key": "ctrl+shift+alt+meta+r",
        "command": "yql.executeYQL"
    },
    {
        "key": "ctrl+shift+alt+meta+s ctrl+shift+alt+meta+e",
        "command": "yandex-code-assist.agent.explainCode"
    },
    {
        "key": "ctrl+shift+alt+meta+s ctrl+shift+alt+meta+f",
        "command": "yandex-code-assist.agent.fixCode"
    },
    {
        "key": "ctrl+shift+alt+meta+s ctrl+shift+alt+meta+t",
        "command": "yandex-code-assist.agent.focusInput"
    },
    {
        "key": "ctrl+shift+alt+meta+s ctrl+shift+alt+meta+i",
        "command": "yandex-code-assist.agent.improveCode"
    },
    {
        "key": "ctrl+shift+alt+meta+f ctrl+shift+alt+meta+f",
        "command": "television.ToggleFileFinder"
    },
    {
        "key": "ctrl+p",
        "command": "-television.ToggleFileFinder"
    },
    {
        "key": "ctrl+shift+alt+meta+f ctrl+shift+alt+meta+g",
        "command": "television.ToggleTextFinder"
    },
    {
        "key": "ctrl+shift+f",
        "command": "-television.ToggleTextFinder"
    },
    {
        "key": "ctrl+shift+alt+meta+f ctrl+shift+alt+meta+s",
        "command": "television.ToggleTextFinderWithSelection"
    },
    {
        "key": "ctrl+shift+alt+meta+w",
        "command": "workbench.action.closeActiveEditor",
        "when": "!editorTextFocus && !filesExplorerFocus && !searchViewletFocus && !terminalFocus && neovim.mode != 'cmdline'"
    },
    {
        "key": "ctrl+w q",
        "command": "-workbench.action.closeActiveEditor",
        "when": "!editorTextFocus && !filesExplorerFocus && !searchViewletFocus && !terminalFocus && neovim.mode != 'cmdline'"
    },
    {
        "key": "ctrl+shift+alt+meta+w",
        "command": "workbench.action.closeActiveEditor"
    },
    {
        "key": "ctrl+w",
        "command": "-workbench.action.closeActiveEditor"
    },
    {
        "key": "ctrl+shift+alt+meta+c",
        "command": "workbench.action.closeUnmodifiedEditors"
    },
    {
        "key": "ctrl+k u",
        "command": "-workbench.action.closeUnmodifiedEditors"
    }
]