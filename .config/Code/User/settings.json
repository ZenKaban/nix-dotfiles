{
    "diffEditor.experimental.showMoves": true,
    "editor.acceptSuggestionOnEnter": "off",
    "workbench.colorTheme": "Solarized Dark",
    "terminal.explorerKind": "external",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "workbench.startupEditor": "none",
    "chat.commandCenter.enabled": false,
    "terminal.integrated.allowChords": false,
    "workbench.settings.openDefaultKeybindings": true,
    "vscode-neovim.compositeKeys": {
        
    }, 
    "code-runner.executorMapByFileExtension": {

        ".yql": "yql --langver 2025.01 -i"
    },
    "code-runner.runInTerminal": true,
    "vscode-neovim.neovimExecutablePaths.linux": "/home/linuxbrew/.linuxbrew/bin/nvim",
    "vscode-neovim.neovimInitVimPaths.linux": "/home/kabanov-alex/.config/nvim/init.lua",
    "editor.lineNumbers": "relative",
    "diffEditor.codeLens": true,
    "terminal.external.linuxExec": "/usr/bin/kitty zsh -ic",
    "terminal.integrated.defaultProfile.linux": "zsh",
    "[yql]": {

        "comments": {
        "lineComment": "--",  // this is wrong
        "blockComment": [
          "/*",
          "*/"
          ]
        },
    },
    "keyboard.dispatch": "keyCode",
    "editor.minimap.autohide": "mouseover",
    "explorer.confirmDelete": false,
    "yandex-code-assist.agents.agentsTestPreview": true,
    "yandex-code-assist.agents.preferredLanguage": "Russian - Русский",
    "yandex-code-assist.agents.chromeExecutablePath": "/usr/bin/yandex-browser",
    "yandex-code-assist.agents.modelSettings.o3Mini.reasoningEffort": "high",
    "window.menuBarVisibility": "compact",
    "code-runner.clearPreviousOutput": true,
    "vscode-yandex-arc.arcPath": "",
    "editor.unicodeHighlight.allowedLocales": {
        "ru": true
    },
    "yandex-code-assist.chat.enableDiffAutoApply": false,
    "yandex-code-assist.chat.preferredLanguage": "Russian - Русский",
    "yandex-code-assist.accumulateContextEnabled": true,
    "yql.enableAutocomplete2": true,
    "yql.disableLinterChecks": [
        "format"
    ],
    "yandex-code-assist.allowedCommands": [
        "npm test",
        "npm install",
        "tsc",
        "git log",
        "git diff",
        "git show"
    ],
    "yandex-code-assist.vsCodeLmModelSelector": {},
    "yandex-code-assist.deniedCommands": [],
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "find-it-faster.findWithinFiles.fuzzRipgrepQuery": true,
    "find-it-faster.findFiles.previewCommand": "bat --color=always {}",
    "find-it-faster.findFiles.previewWindowConfig": "top,50%",
    "find-it-faster.findWithinFiles.previewWindowConfig": "bottom,50%,border-bottom,+{2}+3/3,~3",
    "find-it-faster.general.batTheme": "Solarized (dark)",
    "find-it-faster.general.showMaximizedTerminal": true
}