{
    "diffEditor.experimental.showMoves": true,
    "editor.acceptSuggestionOnEnter": "off",
    "workbench.colorTheme": "Solarized Dark",
    "terminal.explorerKind": "external",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "workbench.startupEditor": "none",
    "chat.commandCenter.enabled": false,
    "terminal.integrated.allowChords": false,
    "workbench.settings.openDefaultKeybindings": true,
    "vscode-neovim.compositeKeys": {
        
    }, 
    "code-runner.executorMapByFileExtension": {

        ".yql": "yql --langver 2025.01 -i"
    },
    "code-runner.runInTerminal": true,
    "vscode-neovim.neovimExecutablePaths.linux": "/home/linuxbrew/.linuxbrew/bin/nvim",
    "vscode-neovim.neovimInitVimPaths.linux": "/home/kabanov-alex/.config/nvim/init.lua",
    "editor.lineNumbers": "relative",
    "diffEditor.codeLens": true,
    "terminal.external.linuxExec": "/usr/bin/kitty zsh -ic",
    "terminal.integrated.defaultProfile.linux": "zsh",
    "[yql]": {

        "comments": {
        "lineComment": "--",  // this is wrong
        "blockComment": [
          "/*",
          "*/"
          ]
        },
    },
    "keyboard.dispatch": "keyCode",
    "editor.minimap.autohide": true,
    "explorer.confirmDelete": false,
    "yandex-code-assist.agents.agentsTestPreview": true,
    "yandex-code-assist.agents.preferredLanguage": "Russian - Русский",
    "yandex-code-assist.agents.chromeExecutablePath": "/usr/bin/yandex-browser",
    "yandex-code-assist.agents.modelSettings.o3Mini.reasoningEffort": "high",
    "window.menuBarVisibility": "compact",
    "code-runner.clearPreviousOutput": true
}